{
  "$schema": "https://raw.githubusercontent.com/subsquid/squid-sdk/master/substrate/substrate-typegen/src/config.schema.json",
  "outDir": "src/types",
  "specVersions": "https://v2.archive.subsquid.io/metadata/phala",
  "pallets": {
    "PhalaPhatContracts": {
      "events": [
        "ClusterCreated",
        "Instantiated",
        "WorkerAddedToCluster",
        "WorkerRemovedFromCluster",
        "UserStakeChanged"
      ]
    },
    "PhalaPhatTokenomic": {
      "events": ["UserStakeChanged"]
    },
    "PhalaComputation": {
      "events": [
        "WorkerStarted",
        "WorkerStopped",
        "WorkerReclaimed",
        "SessionBound",
        "SessionUnbound",
        "WorkerEnterUnresponsive",
        "WorkerExitUnresponsive"
      ]
    },
    "PhalaRegistry": {
      "events": ["WorkerAdded"]
    }
  }
}
